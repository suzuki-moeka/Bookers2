 <%= render 'user', user: current_user %>
 <%= render 'books/new', book: @book_new %>
 
  <% @User.each do |user| %>
    <% if user.profile_image.attached? %>
     <p><%= image_tag user.profile_image(100,100) %></p>
    <% else %>
     <%= image_tag 'no_image', size: "100x100" %>
    <% end %>
    
    <%= user.name%>
    
    <%= link_to "Show", user_path(user.id) %>
  <% end %>
  
  